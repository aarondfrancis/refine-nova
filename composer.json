{
    "name": "hammerstone/refine-nova",
    "description": "A Laravel Nova integration for the Refine query builder.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0",
        "hammerstone/refine-laravel": "^0.3.4",
        "laravel/nova": "^2.12|^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Hammerstone\\Refine\\Nova\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "composer",
            "url": "https://refine.composer.sh"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Hammerstone\\Refine\\Nova\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
